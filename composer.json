{
    "name": "drupal/editorjs",
    "type": "drupal-module",
    "description": "This module integrate EditorJs",
    "keywords": ["Drupal", "EditorJS"],
    "homepage": "https://www.drupal.org/project/editorjs",
    "support": {
        "issues": "https://www.drupal.org/project/issues/editorjs",
        "source": "https://gitlab.com/batkor/editorjs"
    },
    "require": {
        "ext-dom": "*",
        "composer/installers": "~1.0 || ~2.0",
        "oomphinc/composer-installers-extender": "~1.0 || ~2.0",
        "npm-asset/editorjs--checklist": "^1.2",
        "npm-asset/editorjs--code": "^2.6",
        "npm-asset/editorjs--delimiter": "^1.1",
        "npm-asset/editorjs--editorjs": "^2.18",
        "npm-asset/editorjs--header": "^2.5",
        "npm-asset/editorjs--image": "^2.6",
        "npm-asset/editorjs--link": "^2.2",
        "npm-asset/editorjs--list": "^1.6",
        "npm-asset/editorjs--table": "^1.3"
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "extra": {
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "web/libraries/{$name}": ["type:npm-asset"]
        }
    }
}
